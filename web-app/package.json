{
  "name": "tandf-pyramid-oadashboard-web-app",
  "description": "The Pyramid is a T&F project with the goal of creating a single platform that allows customers and consumer to access, read and manage content seamlessly from one T&F interface.",
  "version": "0.1.0",
  "private": true,
  "author": "T&F Technology",
  "license": "UNLICENSED",
  "homepage": "http://confluence.taylorandfrancis.com/display/PYR",
  "repository": {
    "type": "git",
    "url": "http://bitbucket.crcpress.local:7990/scm/IN/tnf-api-dashboard.git"
  },
  "bugs": {
    "url": "http://jira.taylorandfrancis.com/projects/PB/issues"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "scripts": {
    "private-module": "git+ssh://git@bitbucket.org:private/private-module.git",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:uat": "webpack --config config/webpack.uat.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "build:local": "webpack --config config/webpack.local.js --progress --profile",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docker:build": "docker build -t tandf-pyr-cust-web:latest . && docker images",
    "docker:commit": "docker commit -t tandf-pyr-cust-web:latest . && docker images",
    "docker:kill": "docker kill tandf-pyr-cust-web-1 && docker ps",
    "docker:remove": "npm run docker:rm && npm run docker:rmi",
    "docker:rm": "npm run docker:kill && docker rm tandf-pyr-cust-web-1 && docker ps",
    "docker:rmi": "docker rmi tandf-pyr-cust-web:latest --force && docker ps",
    "docker:run": "docker run --name tandf-pyr-cust-web-1 -P -d tandf-pyr-cust-web:latest && docker ps",
    "docker:start": "npm run docker:build && npm run docker:run",
    "docker:update": "npm run docker:remove && npm run docker:start",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "reinstall": "npm run clean && npm install",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:local": "webpack-dev-server --config config/webpack.local.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "start:local": "npm run server:local",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:local": "npm run build:local -- --watch",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
